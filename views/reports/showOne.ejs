<article class="media" id="report_<%= report._id %>">
    <figure class="media-left">
        <p class="image is-64x64">
            <img src="https://bulma.io/images/placeholders/128x128.png">
        </p>
    </figure>
    <div class="media-content">
        <div class="content">
            <p>
                <strong>
                    <%= report.author.username %> - <%= report.title %>
                </strong>
                <br>
                <%= report.description %>
                <br>
                <small>
                    <%= report.createdAt_Date %> -
                    <%= report.createdAt_Time %>
                </small>
            </p>
        </div>
    </div>
</article>